{
  "manifest_version": 3,
  "name": "侧边信息栏",
  "version": "1.0.0",
  "description": "在网页右侧显示信息的简单插件",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_end"
    }
  ]
}

